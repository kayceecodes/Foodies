{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@pages/*": [
        "pages/"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/data/*": [
        "src/components/data/*"
      ],
      "@/src/*": [
        "src/*"
      ],
      "@/theme/*": [
        "src/Theme/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/store/*": [
        "store/*"
      ],
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
